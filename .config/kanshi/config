profile {
  output HDMI-A-1 enable mode 1920x1080@165Hz
  output eDP-1 disable
  exec theme -r
  exec (command -v mako >/dev/null && makoctl reload)
}

profile {
  output eDP-1 enable
  exec theme -r
  exec (command -v mako >/dev/null && makoctl reload)
}

profile {
  output HDMI-A-1 enable mode 1920x1080@165Hz
  exec theme -r
  exec (command -v mako >/dev/null && makoctl reload)
}

profile monitor {
  output HDMI-A-1 enable mode 1920x1080@165Hz
  output eDP-1 disable
  exec theme -r
  exec (command -v mako >/dev/null && makoctl reload)
}

profile laptop {
  output HDMI-A-1 disable
  output eDP-1 enable
  exec theme -r
  exec (command -v mako >/dev/null && makoctl reload)
}

profile dual {
  output HDMI-A-1 enable mode 1920x1080@165Hz
  output eDP-1 enable
  exec theme -r
  exec (command -v mako >/dev/null && makoctl reload)
}
